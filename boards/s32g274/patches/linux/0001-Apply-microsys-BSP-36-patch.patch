+static int m88e1510_unmask_interrupts(struct phy_device *phydev,
+		u16 mask)
+{
+	return phy_write(phydev, MII_PHY_COP_IRQ_EN_REG, mask);
+}
+
+static int m88e1510_mask_interrupts(struct phy_device *phydev,
+		u16 mask)
+{
+	return phy_write(phydev, MII_PHY_COP_IRQ_EN_REG, ~mask);
+}
