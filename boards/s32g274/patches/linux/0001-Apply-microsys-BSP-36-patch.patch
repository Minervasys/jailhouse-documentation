+static const int m88e1510_interrupt_mask(void)
+{
+	return BIT(14)     // Speed Changed
+		| BIT(13)  // Duplex Changed
+		| BIT(11)  // Auto-Negotiation completed
+		| BIT(10); // Link Status Changed
+}
+
+static int m88e1510_did_interrupt(struct phy_device *phydev)
+{
+	int istatus = 0;
+
+	// side-effect: clears pending interrupts
+	istatus = phy_read(phydev, 19);
+
+	if (istatus < 0) return 0;
+
+	if (istatus & m88e1510_interrupt_mask()) return 1;
+
+	return 0;
+}
+
+static irqreturn_t m88e1510_handle_interrupt(struct phy_device *phydev)
+{
+	if (m88e1510_did_interrupt(phydev)) {
+
+		phy_trigger_machine(phydev);
+
+		return IRQ_HANDLED;
+	}
+
+	return IRQ_NONE;
+}
