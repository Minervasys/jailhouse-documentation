+			 gpios = <3 GPIO_ACTIVE_LOW>;
+			 output-high;
+			 line-name = "sja1110-reset-core";
+		 };
+
+		 lin_slp {
+			 gpio-hog;
+			 gpios = <6 GPIO_ACTIVE_HIGH>;
+			 output-low;
+			 line-name = "lin-sleep";
+		 };
+
+	};
