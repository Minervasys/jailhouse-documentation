+/*!
+ * \internal
+ *****************************************************************************
+ * Function: m88e151x_disable_rgmii_clk()
+ *
+ * \brief Disables RGMII clocking
+ *
+ * If the PHY interface is _not_ RGMII then the RGMII interface is
+ * disabled in the PHY. This includes the 125MHz clock for RGMII as
+ * well as the pin output of the 125MHz clock.
+ *
+ * In case of having an RGMII interface the pin output of the 125MHz
+ * clock is disabled, except if the device tree property
+ * "rgmii,clk125-output-enable" is given. In the latter case the pin
+ * output is configured.
+ *
+ * Note that a software reset of the PHY is necessary to make the
+ * changes effective.
+ *
+ * Side effect: the register page is switched to page 2.
+ *
+ * @param phydev pointer to PHY device structure
+ * @return errno 0 on success, otherwise error code
+ *
+ * @author kpotthoff
+ * @date 14.12.2023
+ *****************************************************************************
+ */
+static int m88e151x_disable_rgmii_clk(struct phy_device *phydev)
+{
+	u16 reg;
+	int rv;
+
+	const bool is_rgmii_if = phy_interface_is_rgmii(phydev);
+
+	/*
+	 * Disable RGMII clocking:
+	 */
+
+	// switch to page 2
+	rv = marvell_set_page(phydev, MII_MARVELL_MSCR_PAGE);
+	if (rv)
+		return rv;
+
+	// read MAC Specific Control Register 1 (page 2, register 16)
+	reg = phy_read(phydev, MDIO_DEVAD_NONE, MIIM_88E151x_PHY_MSCR1);
+
+	if (is_rgmii_if) {
+		const bool clk125_output_enable
+			= ofnode_read_bool(phydev->node,
+					"rgmii,clk125-output-enable");
+
+		if (clk125_output_enable)
+			reg &= ~MIIM_88E151x_CLK125_OUT_DIS; // == don't disable
+		else
+			reg |= MIIM_88E151x_CLK125_OUT_DIS;
+	}
+	else {
+		// disables clock output and internally generated 125MHz:
+		reg |= MIIM_88E151x_CLK125_DIS | MIIM_88E151x_CLK125_OUT_DIS;
+
+		// powers down RGMII interface and disables RCLK:
+		reg &= ~(MIIM_88E151x_RGMII_IF_DOWN | MIIM_88E151x_RCLK_EN);
+	}
+
+	// Note: a software reset is needed to make the changes
+	// take effect.
+	rv = phy_write(phydev, MDIO_DEVAD_NONE, MIIM_88E151x_PHY_MSCR1, reg);
+
+	return rv;
+}
