+	switch (media) {
+	case S32G_BOOT_QSPI:
+		puts("Boot:  QSPI\n");
+		break;
+	case S32G_BOOT_SD:
+		puts("Boot:  SD\n");
+		break;
+	case S32G_BOOT_EMMC:
+		puts("Boot:  eMMC\n");
+		break;
+	}
+
+	return 0;
+}
